{
    "scopeName": "source.timeline.id-expression.thaliak",
    "injectionSelector": "L:source",
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "patterns": [{ "include": "#id-expression" }],
    "repository": {
        "id-expression": {
            "match": "(?<=id:\\s)([A-z<][A-z0-9\\-_\\(\\)<>]+)",
            "captures": {
                "1": { "name": "entity.name.variable.template.thaliak" }
            }
        }
    }
}
